						WriteLiteral(" ......................................................................................");
					}
					else
					{
						<span style="font-weight:bold;">
							@Model.Localidad
						</span>
					}
					Código Postal:
					@if (@Model.PostalCode == null)
					{
						WriteLiteral(" .......................");
					}
					else
					{
						<span style="font-weight:bold;">
							@Model.PostalCode
						</span>
					}
				</p>
			</li>
			<li>
				<p class="datos-alumno">
