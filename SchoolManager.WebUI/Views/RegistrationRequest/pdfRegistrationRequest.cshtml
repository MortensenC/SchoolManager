@model SchoolManager.Domain.Entities.RegistrationRequest
<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<title>SOLICITUD DE INGRESO</title>
	<style>
		html {
			/*margin: 0 0 0 0;*/
            font-family: 'Times New Roman'; 
		}
        body {
            margin: 30px 30px 20px 30px;
        }
		div {
			float: left;
			width:  100%;
		}
        p {
            display: block;
            /*display: inline-block;*/
            margin: 0;
        }
		h3, h4, h5 {
			margin-top: 0;
			margin-bottom: 0;	
			display: inline-block;
		}
        ul {
            /*margin-top: 0;
            margin-bottom: 0;*/
            margin: 0px;
            /*padding: 0px;*/
            padding-left: 10px;
            list-style-position: inside;
        }
		ol {
			margin: 0;
			padding: 10px;
			list-style-position: inside;
		}

        ol li {
            margin: 5px 0;
        }

		span {
			margin: 5px 0;
		}
		
		/*titulo-general*/
        div.titulo-demarcado {
            border-top: 1px solid black;
            border-bottom: 1px solid black;
            vertical-align: middle;
        }
		div.titulo-demarcado h2 {
			text-align:  center;
            vertical-align: middle;
		}
        div.titulo-tema {
            margin-top: 5px;
            margin-bottom: 5px;
        }

        table {
            width: 100%;
            font-size: 9pt;
            border-collapse: collapse;
        }

        td.celda {
            border: 1px solid black;
            padding: 0 0 0 0.01in;
        }

        p.datos-alumno {
            font-size: 16px;
        }

        div.mother p, div.father p, div.tutor p {
            line-height: 140%;
            font-size: 16px;
            width: 100%;
        }

		br {
            padding: 0;
            margin: 0;
            display: block;
		}

        div.inblock p {
            display: inline-block;
        }

        div.lineaDivisoria {
            border-bottom: 1px solid #000000;
            height: 1px;
            margin-bottom: 10px;
            margin-top: 10px;
        }
	</style>
</head>
<body>
