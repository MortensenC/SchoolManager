@model SchoolManager.Domain.Entities.User
@{
    ViewBag.Title = "Eliminar";
}
<div class="banner-seccion">
	<img src="@Url.Content("~/Content/Images/banner_talleres.jpg")" />
</div>
<div class="row-donde">
    <div class="content-wrapper">
        <p>
            Estás aquí: <span>Admin</span> > <span>Usuarios</span> > <span class="hlited">Eliminar</span></p>
    </div>
</div>
<div class="content-wrapper seccion clear-fix">
    <h1 class="header-seccion">
        Eliminar usuario</h1>
    @using (Html.BeginForm())
    {
        @:Está seguro que desea eliminar el usuario <b>@Model.FullName</b>?
        <br />
        <br />
        <p>
            <input class="regularbtn float-left" type="submit" value="Eliminar" />
            <div class="secbtn padfix"><a href="javascript:history.back()">Volver al listado</a></div>
        </p>
    }
</div>